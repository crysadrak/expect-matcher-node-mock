{
  "name": "expect-matcher-node-mock",
  "version": "1.1.3",
  "description": "Jest-like expect matchers for native Node.js test runner mock objects. Provides toHaveBeenCalled, toHaveBeenCalledWith, toHaveReturned and more.",
  "main": "lib/index.mjs",
  "type": "module",
  "homepage": "https://github.com/crysadrak/expect-matcher-node-mock#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "jest",
    "expect",
    "matcher",
    "mock",
    "node",
    "test",
    "testing",
    "node-test",
    "nodejs",
    "test-runner",
    "unit-test",
    "assertion",
    "spy",
    "stub"
  ],
  "bugs": {
    "url": "https://github.com/crysadrak/expect-matcher-node-mock/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crysadrak/expect-matcher-node-mock"
  },
  "license": "MIT",
  "author": "Crysa Drak <cd@l33t.cz>",
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "node --test --test-reporter=spec"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "strip-ansi": "7.1.0"
  },
  "peerDependencies": {
    "chalk": ">=4.0.0",
    "expect": ">=29.0.0",
    "jest-matcher-utils": ">=29.0.0"
  }
}
